{
    "coverage": {
        "US Census": {
            "geoid": "22033",
            "name": "East Baton Rouge Parish",
            "state": "Louisiana"
        },
        "country": "us",
        "state": "la",
        "county": "East Baton Rouge"
    },
    "data": "https://maps.brla.gov/gis/rest/services/Map_Reference/Street_Address/MapServer/0",
    "type": "ESRI",
    "conform": {
        "type": "geojson",
        "number": {
            "function": "prefixed_number",
            "field": "FULL_ADDRESS"
        },
        "street": {
          "function": "regexp",
          "field": "FULL_ADDRESS",
          "pattern": "^(?:\\d+(?:-\\d+)?)\\s+(?:(?:STE|BLDG|UNIT|APT) \\d+)?\\s+.+"
        },
        "unit": {
          "function": "regexp",
          "field": "FULL_ADDRESS",
          "pattern": "\b((?:STE|BLDG|UNIT|APT) \\d+)\b"
        },
        "city": "CITY",
        "region": "STATE",
        "postcode": "ZIP"
    },
    "test": {
        "enabled": true,
        "description": "these are the tests that exercise valid inputs for czech countrywide",
        "acceptance-tests": [
            {
                "description": "address w/o unit",
                "inputs": {
                    "FULL_ADDRESS": "455 BELLEWOOD DR",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "455",
                    "street": "BELLEWOOD DR",
                    "unit": ""
                }
            },
            {
                "description": "address with hyphenated number",
                "inputs": {
                    "FULL_ADDRESS": "1600-1700 UND LOBDELL BLVD",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "1600-1700",
                    "street": "UND LOBDELL BLVD",
                    "unit": ""
                }
            },
            {
                "description": "address with numeric suite",
                "inputs": {
                    "FULL_ADDRESS": "383 STE 217 HIGHLANDIA DR",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "383",
                    "street": "HIGHLANDIA DR",
                    "unit": "STE 217"
                }
            },
            {
                "description": "address with hyphenated numeric suite",
                "inputs": {
                    "FULL_ADDRESS": "2156 STE 140-150 WOODDALE BLVD",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "2156",
                    "street": "WOODDALE BLVD",
                    "unit": "STE 140-150"
                }
            },
            {
                "description": "address with alpha suite",
                "inputs": {
                    "FULL_ADDRESS": "12333 STE B JEFFERSON HWY",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "12333",
                    "street": "JEFFERSON HWY",
                    "unit": "STE B"
                }
            },
            {
                "description": "address with alphanumeric suite",
                "inputs": {
                    "FULL_ADDRESS": "1075 STE D15 EXECUTIVE PARK AVE",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "1075",
                    "street": "EXECUTIVE PARK AVE",
                    "unit": "STE D15"
                }
            },
            {
                "description": "address with alpha building",
                "inputs": {
                    "FULL_ADDRESS": "7380 BLDG C EXCHANGE PL",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "7380",
                    "street": "EXCHANGE PL",
                    "unit": "BLDG C"
                }
            },
            {
                "description": "address with numeric building",
                "inputs": {
                    "FULL_ADDRESS": "6444 BLDG 22 JONES CREEK RD",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "6444",
                    "street": "JONES CREEK RD",
                    "unit": "BLDG 22"
                }
            },
            {
                "description": "address with numeric apartment",
                "inputs": {
                    "FULL_ADDRESS": "11440 APT# 442 BARD AVE",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "11440",
                    "street": "BARD AVE",
                    "unit": "APT# 442"
                }
            },
            {
                "description": "address with numeric apartment",
                "inputs": {
                    "FULL_ADDRESS": "10954 #C JOOR RD",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "10954",
                    "street": "JOOR RD",
                    "unit": "#C"
                }
            },
            {
                "description": "address with numeric unit",
                "inputs": {
                    "FULL_ADDRESS": "260 UNIT #33 SHARP RD",
                    "CITY": "",
                    "STATE": "",
                    "ZIP": ""
                },
                "expected": {
                    "number": "260",
